<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#541B16"
    tools:context=".PageInscription">


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="501dp"
        android:layout_height="777dp"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="16dp"
        android:background="#541B16"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.518"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/page_inscription" />

    <EditText
        android:id="@+id/url_cv"
        android:layout_width="270dp"
        android:layout_height="30dp"
        android:layout_marginStart="68dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.713" />

    <Button
        android:id="@+id/inscriptionRetour"
        android:layout_width="165dp"
        android:layout_height="37dp"
        android:alpha="0"
        android:background="@color/black"
        android:text="retour"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.455"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.885"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/buttonInscription"
        android:layout_width="307dp"
        android:layout_height="42dp"
        android:alpha="0"
        android:background="@color/black"
        android:text="cree compte"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView4"
        app:layout_constraintVertical_bias="0.806"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/motDePasseInscription"
        android:layout_width="264dp"
        android:layout_height="47dp"
        android:ems="10"
        android:hint="Entrez votre mot de passe"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.394"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.615"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/emailInscription"
        android:layout_width="268dp"
        android:layout_height="43dp"
        android:ems="10"
        android:hint="Entrez votre courriel"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.405"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.533"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/prenomInscription"
        android:layout_width="270dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Entrez votre votre prÃ©nom"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.368"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.444"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/nomInscription"
        android:layout_width="267dp"
        android:layout_height="54dp"
        android:ems="10"
        android:hint="Entrez votre nom"
        android:inputType="text"
        android:minHeight="48dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.361"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.339" />


</androidx.constraintlayout.widget.ConstraintLayout>